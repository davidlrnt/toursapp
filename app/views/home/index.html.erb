  <%= form_for :search, url: searches_path  do |f| %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :category %>
    <%= f.collection_select :category, Category.all, :name, :name %>
    <%= f.label :tags %>
    <%= f.text_field :tags %>
    <%= f.submit "Find" %>
  <% end %>
